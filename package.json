{
    "name": "gemini-schedule-demo",
    "version": "1.0.0",
    "description": "A demo application showing how to use Google's Gemini API to create student schedules",
    "main": "dist/dayplanner-tests.js",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/itinerary-tests.js",
        "dev": "ts-node --transpile-only itinerary-tests.ts",
        "test": "npm run build && node dist/itinerary-tests.js",
        "manual": "npm run build && node -e \"require('./dist/itinerary-tests.js').testManualItinerary()\"",
        "llm": "npm run build && node -e \"require('./dist/itinerary-tests.js').testLLMSuggestions()\"",
        "ambiguous": "npm run build && node -e \"require('./dist/itinerary-tests.js').testAmbiguousDestination()\"",
        "ambiguous-empty": "npm run build && node -e \"require('./dist/itinerary-tests.js').testAmbiguousDestinationEmpty()\"",
        "lowbudget": "npm run build && node -e \"require('./dist/itinerary-tests.js').testLowBudget()\"",
        "full": "npm run build && node -e \"require('./dist/itinerary-tests.js').testFullItineraryConflict()\""
    },
    "keywords": [
        "gemini",
        "ai",
        "schedule",
        "student",
        "demo",
        "typescript"
    ],
    "author": "Student Demo",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.21.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0",
        "ts-node": "^10.9.0"
    }
}
